# ---------- Miscellaneous Bindings
# UNBINDS
unmap q

# BINDS
map 0 exec "xdotool key ctrl+d"
map 9 exec "xdotool key ctrl+u"
map <Button2> navigate previous
map <Button3> navigate next
map <C-[> feedkeys ":nohlsearch<Return>"
map <Esc> feedkeys ":nohlsearch<Return>"
map <Space> feedkeys ":nohlsearch<Return>"
map \\ feedkeys "gk"
map [ exec "winstack prev"
map ] exec "winstack next"
map = zoom in
map ; feedkeys ":"
map ,b focus_inputbar ":bmark "
map d feedkeys ":set first-page-column 1:2<Return>"
map ,d feedkeys ":bdelete "
map D feedkeys ":set first-page-column 1:1<Return>"
map <C-f> toggle_fullscreen
map [fullscreen] <C-f> toggle_fullscreen
map ,g focus_inputbar ":blist "  # List Marks
map h navigate previous
map l navigate next
map ,n exec "zsearch --new-workspace --no-search"
map o exec "zopen -x"
map ,o exec "zopen -C -x"
map O exec "zopen"
map ,O exec "zopen -C"
map p feedkeys ":set pages-per-row 1<Return>"
map P feedkeys ":set pages-per-row 2<Return>"
map ,p feedkeys ":set pages-per-row 3<Return>"
map R exec "zopen -R"
map x exec "winclose"
map Z feedkeys "200zz"

