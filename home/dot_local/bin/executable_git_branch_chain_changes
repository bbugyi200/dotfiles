#!/bin/bash
# Show files that were modified by the 19 commits BEFORE the LAST commit

set -e

# Get files changed in the 19 commits before HEAD (HEAD~19..HEAD~1)
# This excludes the last commit and goes back 19 commits from there
git diff --name-only HEAD~19..HEAD~1 2>/dev/null || {
    # If we don't have 19 commits, just show all commits except the last one
    git diff --name-only HEAD~1 2>/dev/null || true
}
